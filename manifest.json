  {
    "name": "FastPaste",
    "version": "0.1.0",
    "description": "Tool to allow users to use Copy & Paste more efficiently!",
    "manifest_version": 2,
    "content_scripts": [
    {

    	"matches": ["<all_urls>"],
    	"js": ["content.js"]
    }


    ],

    "browser_action":
    {
    	"default_icon": "picture.png",
    	"default_title": "Click here for more options"


    }

  }